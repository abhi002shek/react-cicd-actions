{
  "dashboard": {
    "id": null,
    "title": "React App Monitoring",
    "tags": ["react", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "ðŸ’– Total Likes",
        "type": "stat",
        "targets": [
          {
            "expr": "app_likes_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "ðŸ‘€ Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "app_page_views_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "ðŸ“ˆ Likes Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "app_likes_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 5,
              "showPoints": "always"
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1
  }
}
