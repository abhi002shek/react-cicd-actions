{
  "dashboard": {
    "id": null,
    "title": "React App Monitoring - Beginner Dashboard",
    "tags": ["react", "monitoring", "beginner"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "ðŸ’– Total Likes",
        "type": "stat",
        "targets": [
          {
            "expr": "app_likes_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            },
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "ðŸ‘€ Total Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "app_page_views_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0}
      },
      {
        "id": 3,
        "title": "ðŸ“ˆ Likes Activity (Last Hour)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "app_likes_total",
            "refId": "A",
            "legendFormat": "Total Likes"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 5,
              "showPoints": "always"
            }
          }
        },
        "gridPos": {"h": 8, "w": 16, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "ðŸš€ Likes Per Minute",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(app_likes_total[5m]) * 60",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "short",
            "decimals": 1,
            "thresholds": {
              "steps": [
                {"color": "blue", "value": null},
                {"color": "green", "value": 1},
                {"color": "yellow", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1
  }
}
